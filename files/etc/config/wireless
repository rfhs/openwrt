config wifi-iface
	option device 'radio1b'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WPA_11r_2G'
	option encryption 'psk2+ccmp'
	option key 'This is only a test'
	option ieee80211r '1'
	option nasid 'RFHS'
	option mobility_domain '1337'
	option r0_key_lifetime '65535'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'


config wifi-device 'radio0a'
	option type 'mac80211'
	option hwmode '11a'
	option phy 'phy0'
	option country 'US'
	option country_ie '1'
	option channel '44'
	option htmode 'VHT80'
	option legacy_rates '0'

config wifi-device 'radio0b'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option phy 'phy1'
	option htmode 'HT20'
	#This treats all non-802.11n as noise, probably bad
	#option greenfield '1'
	option country 'US'
	option country_ie '1'
	option legacy_rates '0'

config wifi-device 'radio1a'
	option type 'mac80211'
	option hwmode '11a'
	option phy 'phy2'
	option country 'US'
	option country_ie '1'
	option channel '44'
	option htmode 'VHT80'
	option legacy_rates '0'

config wifi-device 'radio1b'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option phy 'phy3'
	option htmode 'HT20'
	#This treats all non-802.11n as noise, probably bad
	#option greenfield '1'
	option country 'US'
	option country_ie '1'
	option legacy_rates '0'

config wifi-iface
	option device 'radio0b'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WEP64_2G'
	option encryption 'wep+shared'
	option key '1'
	option key1 'FEEDC0FFEE'
	option key2 'FEEDC0FFEE'
	option key3 'FEEDC0FFEE'
	option key4 'FEEDC0FFEE'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'

config wifi-iface
	option device 'radio0a'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WEP64_5G'
	option encryption 'wep+mixed'
	option key '1'
	option key1 'FEEDC0FFEE'
	option key2 'FEEDC0FFEE'
	option key3 'FEEDC0FFEE'
	option key4 'FEEDC0FFEE'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'

config wifi-iface
	option device 'radio0b'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WEP128_2G'
	option encryption 'wep+open'
	option key '1'
	option key1 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option key2 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option key3 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option key4 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'

config wifi-iface
	option device 'radio0a'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WEP128_5G'
	option encryption 'wep+open'
	option key '1'
	option key1 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option key2 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option key3 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option key4 '0FEEDDEADBEEFFEEDDEADBEEF0'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'

config wifi-iface
	option device 'radio0b'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WPA_2G'
	option encryption 'psk+tkip'
	option key 'This is only a test'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'

config wifi-iface
	option device 'radio0a'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WPA_5G'
	option encryption 'psk+tkip+ccmp'
	option key 'This is only a test'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'

config wifi-iface
	option device 'radio0b'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WPA_HIDDEN_2G'
	option encryption 'psk-mixed+tkip+ccmp'
	option key 'This is only a test'
	option isolate '1'
	option hidden '1'
	option maxassoc '50'
	option network 'Admin42'

config wifi-iface
	option device 'radio0a'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WPA_HIDDEN_5G'
	option encryption 'psk-mixed+tkip+ccmp'
	option key 'This is only a test'
	option isolate '1'
	option hidden '1'
	option maxassoc '50'
	option network 'Admin42'


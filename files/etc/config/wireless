config wifi-iface
	option device 'radio1b'
	option mode 'ap'
	option dtim_period '1'
	option ssid 'RFHS_WPA_11w1_2G'
	option encryption 'psk2+tkip+ccmp'
	option key 'This is only a test'
	option ieee80211w '1'
	option isolate '1'
	option maxassoc '50'
	option network 'Admin42'

